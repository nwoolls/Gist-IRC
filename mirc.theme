VERSION=gist irc
LISTCOLOR=#FFFFFF
LISTTEXTCOLOR=#000000
CHATCOLOR=#FFFFFF
CHATTEXTCOLOR=#000000
TOPICCOLOR=#FFFFFF
TOPICTEXTCOLOR=#000000
PROMPT=*
TIME=[HH:MM]
PRIVMSG=5$time -$nick- $indent$text
MYPRIVMSG=$time -> *$target* $indent$text
CHANMSG=$time <$sign$nick> $indent$text
JOIN=3$time $prompt $indent$nick ($host) has joined $target
PART=14$time $prompt $indent$nick ($host) has left $target $text
QUIT=2$time $prompt $indent$nick ($host) Quit ($text)
SERVERNOTICE=5$time -$nick- $indent$text
NOTICE=5$time -$nick- $indent$text
MYNOTICE=$time -> *$target* $indent$text
CHANMODE=3$time $prompt $indent$nick sets mode: $text
USERMODE=3$time $prompt $indent$nick sets mode: $text
307=$time $nick $text
311=$time $nick is $host * $text
312=$time $nick using $text $text2
317=$time $nick has been idle $text, signed on $text2
318=$time $nick End of /WHOIS list.
319=$time $nick on $text
MYCTCPQUERY=4$time -> [$target] $text
CTCPQUERY=4$time [$nick $text]
CTCPREPLY=4$time [$nick $text]: $text2
NICK=3$time $prompt $indent$nick is now known as $nick2
TOPIC=3$time $prompt $indent$nick changes topic to '$text'
ACTION=6$time $prompt $sign$nick $indent$text
KICK=3$time $prompt $indent$nick2 was kicked by $nick ($text)
TEXT=$time $prompt $TEXT
CHANTITLE=$target ($network, $nickname)
SERVERTITLE=$nickname on $network ($server:$port)
RAWIN=< $text
RAWOUT=> $text